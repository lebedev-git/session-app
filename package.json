{
  "name": "session-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\"",
    "dev:server": "node server/index.js",
    "dev:frontend": "cd frontend && npx vite --host",
    "build": "cd frontend && npm install && npx vite build",
    "start": "node server/index.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1",
    "node-telegram-bot-api": "^0.66.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
